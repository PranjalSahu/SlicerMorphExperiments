<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Examples</category>
  <index>0</index>
  <title>ITKALPACA</title>
  <description><![CDATA[Apply a Gaussian blur to an image]]></description>
  <version>0.1.0.</version>
  <documentation-url>https://github.com/username/project</documentation-url>
  <license/>
  <contributor>Andras Lasso (PerkLab)</contributor>
  <acknowledgements><![CDATA[This work is part of the National Alliance for Medical Image Computing (NAMIC), funded by the National Institutes of Health through the NIH Roadmap for Medical Research, Grant U54 EB005149.]]></acknowledgements>
  <parameters>
    <double>
      <name>subsample_radius</name>
      <label>PointSet Subsample Radius</label>
      <index>3</index>
      <description><![CDATA[Radius used to sub-sample points from full resolution pointset]]></description>
      <default>4.5</default>
    </double>
    <integer>
      <name>number_of_ransac_points</name>
      <label>Ransac Subset Points</label>
      <index>4</index>
      <description><![CDATA[Number of points to use in each RANSAC iteration]]></description>
      <default>100</default>
    </integer>
    <double>
      <name>inlier_value</name>
      <label>RANSAC Inlier Value</label>
      <index>5</index>
      <description><![CDATA[Inlier threshold to determine Feature Matching]]></description>
      <default>20.0</default>
    </double>
    <integer>
      <name>fpfh_radius</name>
      <label>FPFH Radius</label>
      <index>6</index>
      <description><![CDATA[FPFH Feature Radius]]></description>
      <default>25</default>
    </integer>
    <integer>
      <name>fpfh_neighbors</name>
      <label>Max FPFH Neighbors</label>
      <index>7</index>
      <description><![CDATA[FPFH Max Number of Neihbors]]></description>
      <default>100</default>
    </integer>
    <double>
      <name>deform_sigma</name>
      <label>Sigma for Deformable</label>
      <index>8</index>
      <description><![CDATA[Sigma for Deformable Registration]]></description>
      <default>3.0</default>
    </double>
    <integer>
      <name>deform_neighbourhood</name>
      <label>Neighbors for Deformable</label>
      <index>9</index>
      <description><![CDATA[Number of neighbors for Deformable Registratation]]></description>
      <default>20</default>
    </integer>
    <integer>
      <name>bspline_grid</name>
      <label>BSpline Grid</label>
      <index>10</index>
      <description><![CDATA[Number of Grids in the BSpline transform for Deformable Registration]]></description>
      <default>5</default>
    </integer>
    <integer>
      <name>deformable_iterations</name>
      <label>Deformable Iterations</label>
      <index>11</index>
      <description><![CDATA[Number of iterations for Deformable Registration Step]]></description>
      <default>100</default>
    </integer>
    <integer>
      <name>ransac_iterations</name>
      <label>RANSAC Iterations</label>
      <index>12</index>
      <description><![CDATA[Number of iterations for RANSAC]]></description>
      <default>50000</default>
    </integer>
    <double>
      <name>alpha_parameter</name>
      <label>Alpha Parameter</label>
      <index>13</index>
      <description><![CDATA[Alpha Parameter]]></description>
      <default>0.1</default>
    </double>
     <double>
      <name>beta_parameter</name>
      <label>Beta Parameter</label>
      <index>14</index>
      <description><![CDATA[Beta Parameter]]></description>
      <default>20.0</default>
    </double>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>
    <geometry>
      <name>targetMesh</name>
      <label>Target Mesh</label>
      <channel>input</channel>
      <index>0</index>
      <description><![CDATA[Target Mesh]]></description>
    </geometry>
    <geometry>
      <name>sourceMesh</name>
      <label>Source Mesh</label>
      <channel>input</channel>
      <index>1</index>
      <description><![CDATA[Source Mesh]]></description>
    </geometry>
    <string>
      <name>inputLandmarks</name>
      <label>Input Landmarks</label>
      <channel>input</channel>
      <index>15</index>
      <description><![CDATA[Input Landmarks]]></description>
    </string>
  </parameters>
</executable>
